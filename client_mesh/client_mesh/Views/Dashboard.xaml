<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	xmlns:local="clr-namespace:client_mesh"
    xmlns:client_mesh_ViewModel="clr-namespace:client_mesh.ViewModels"
	x:Class="client_mesh.Dashboard"
	d:DesignWidth="640" d:DesignHeight="480">
	<UserControl.Resources>
        <client_mesh_ViewModel:DashboardModel x:Key="DashboardModelDataSource" />
	</UserControl.Resources>

	<Grid x:Name="LayoutRoot" DataContext="{Binding Source={StaticResource DashboardModelDataSource}}">
		<TextBlock HorizontalAlignment="Center" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Center" FontSize="96"/>
	</Grid>
</UserControl>
